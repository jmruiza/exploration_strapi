{
  "kind": "collectionType",
  "collectionName": "Projects",
  "info": {
    "name": "Project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 120,
      "unique": true
    },
    "customerTittle": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "collaborators": {
      "via": "project",
      "collection": "collaborator-role"
    },
    "customer": {
      "via": "projects",
      "model": "customer"
    },
    "durationDays": {
      "type": "integer",
      "private": false,
      "default": 0
    },
    "durationMonths": {
      "type": "integer",
      "private": false,
      "default": 0
    },
    "operationalCost": {
      "type": "decimal",
      "private": false,
      "default": 0
    },
    "collaboratorsCost": {
      "type": "decimal",
      "private": false,
      "default": 0
    },
    "profit": {
      "type": "decimal",
      "private": false,
      "default": 0
    },
    "taxISR": {
      "type": "decimal",
      "private": false,
      "default": 0
    },
    "subtotal": {
      "type": "decimal",
      "private": false,
      "default": 0
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "stopped_canceled",
        "completed",
        "in_process",
        "paused",
        "proposal"
      ],
      "default": "proposal"
    },
    "activities": {
      "via": "project",
      "collection": "activity"
    }
  }
}
